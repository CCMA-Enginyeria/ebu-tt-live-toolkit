<?xml version="1.0" ?>
<tt:tt ebuttp:sequenceIdentifier="testSequence001"
       ebuttp:sequenceNumber="1"
       ttp:clockMode="local"
{% if cell_resolution and cell_resolution != "*?Empty?*" %}
       ttp:cellResolution="{{ cell_resolution }}"
{% endif %}
{% if extent and extent != "*?Empty?*" %}
       tts:extent="{{ extent }}"
{% endif %}
       ttp:timeBase="clock"
       xml:lang="en-GB"
       xmlns:ebuttm="urn:ebu:tt:metadata"
       xmlns:ebuttp="urn:ebu:tt:parameters"
       xmlns:tt="http://www.w3.org/ns/ttml"
       xmlns:ttp="http://www.w3.org/ns/ttml#parameter"
       xmlns:tts="http://www.w3.org/ns/ttml#styling"
       xmlns:xml="http://www.w3.org/XML/1998/namespace">
  <tt:head>
    <tt:metadata>
      <ebuttm:documentMetadata/>
    </tt:metadata>
    <tt:styling>
      <tt:style xml:id="S1"
        {% if S1_value and S1_value != "*?Empty?*" %} {{ style_attribute }}="{{ S1_value }}" {% endif %} />
      <tt:style xml:id="S2"          
        {% if S2_value and S2_value != "*?Empty?*" %} {{ style_attribute }}="{{ S2_value }}" {% endif %} 
        {% if S3_value and S3_value != "*?Empty?*" %} {{ other_style_attribute }}="{{ other_style_value }}" {% endif %}/>
    </tt:styling>
    </tt:styling>
    <tt:layout>
      <tt:region xml:id="R1"/>
    </tt:layout>
  </tt:head>
  <tt:body begin="00:00:00.500" dur="00:00:05">
    <tt:div region="R1">
      <tt:p xml:id="ID005">
        <tt:span style="S2" begin="00:00:01" end="00:00:02" xml:id="{{ elem_id }}">Some example text...</tt:span>
      </tt:p>
    </tt:div>
  </tt:body>
</tt:tt>
