          {{body_facet_term}}
      </ebuttm:facet>
    </tt:metadata>
{% endif %}
    <tt:div>
{% if div_facet_term %}
      <tt:metadata>
        <ebuttm:facet
  {% if div_facet_link %}
    {% if div_facet_link == "*?Empty?*" %}
