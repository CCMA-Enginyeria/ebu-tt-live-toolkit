  {% endif %}
  {% if body_facet_expresses %}
    {% if body_facet_expresses == "*?Empty?*" %}
        tt:expresses=""
    {% else %}
        tt:expresses="{{body_facet_expresses}}"
    {% endif %}
  {% endif %}
