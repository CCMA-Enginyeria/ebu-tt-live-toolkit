              {{p_facet_term}}
            </ebuttm:facet>
          </tt:metadata>
{% endif %}
          <tt:span>
{% if span_facet_term %}
            <tt:metadata>
              <ebuttm:facet
  {% if span_facet_link %}
    {% if span_facet_link == "*?Empty?*" %}
