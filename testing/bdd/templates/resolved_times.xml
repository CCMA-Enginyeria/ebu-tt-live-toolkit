<?xml version="1.0" ?>
<tt:tt
        ebuttp:sequenceIdentifier="testSequence001"
        ebuttp:sequenceNumber="{{sequence_number}}"
        ttp:timeBase="{{ time_base }}"
{% if time_base == 'smpte' %}
        ttp:frameRate="25"
{% endif %}
{% if time_base == 'clock' %}
        ttp:clockMode='local'
{% endif %}
        xml:lang="en-GB"
        xmlns:ebuttm="urn:ebu:tt:metadata"
        xmlns:ebuttp="urn:ebu:tt:parameters"
        xmlns:tt="http://www.w3.org/ns/ttml"
        xmlns:ttp="http://www.w3.org/ns/ttml#parameter"
        xmlns:xml="http://www.w3.org/XML/1998/namespace">
  <tt:head>
    <tt:metadata>
      <ebuttm:documentMetadata/>
    </tt:metadata>
    <tt:styling>
      <tt:style xml:id="ID001"/>
    </tt:styling>
    <tt:layout/>
  </tt:head>
  <tt:body 
{% if body_begin %}
  {% if body_begin == "*?Empty?*" %}
      tt:begin=""
  {% else %}
      tt:begin="{{ body_begin }}"
  {% endif %}
{% endif %}
{% if body_end %}
  {% if body_end == "*?Empty?*" %}
      tt:end=""
  {% else %}
      tt:end="{{ body_end }}"
  {% endif%}
{% endif %}
{% if body_dur %}
  {% if body_dur == "*?Empty?*" %}
      tt:dur=""
  {% else %}
      tt:dur="{{ body_dur }}"
  {% endif %}
{% endif %}
    >
    <tt:div>
      <tt:p xml:id="ID005">
            <tt:span>
              Some example text...
            </tt:span>
        <tt:br/>
        <tt:span>And another line</tt:span>
      </tt:p>
    </tt:div>
  </tt:body>
