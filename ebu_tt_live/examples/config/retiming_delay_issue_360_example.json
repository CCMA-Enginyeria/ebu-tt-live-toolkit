{
  "nodes": {
    "node1": {
      "id": "producer1",
      "type": "simple-producer",
      "show_time": true,
      "sequence_identifier": "TestSequence1",
      "output": {
        "carriage": {
          "type": "websocket",
          "listen": "ws://localhost:9000"
        }
      }
    },
    "node2": {
      "id": "producer1",
      "type": "simple-producer",
      "show_time": false,
      "sequence_identifier": "TestSequence2",
      "output": {
        "carriage": {
          "type": "websocket",
          "listen": "ws://localhost:9001"
        }
      }
    },
    "node3": {
      "id": "retiming-delay1",
      "type": "retiming-delay",
      "delay": 2,
      "sequence_identifier": "RetimedSequence1",
      "input": {
        "carriage": {
          "type": "websocket",
          "connect": [
            "ws://localhost:9000/TestSequence1/subscribe",
            "ws://localhost:9001/TestSequence2/subscribe"
            ]
        }
      },
      "output": {
        "carriage": {
          "type": "websocket",
          "listen": "ws://localhost:9002"
        }
      }
    },
    "node4": {
      "id": "consumer1",
      "type": "simple-consumer",
      "input": {
        "carriage": {
          "type": "websocket",
          "connect": [
            "ws://localhost:9002/RetimedSequence1/subscribe"
            ]
        }
      }
    }
  }
}
